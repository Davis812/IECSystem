<%- include header%>
    <div id="mainContent">
      <table class="table table-striped">
    	<thead>
            <tr>
                <th width="50">序号</th>
                <th>账号</th>
                <th>电话</th>
                <th>权限</th>
                <th>管理</th>
            </tr>
        </thead>
 		<tbody>
            <% body.forEach(function(user,index){%>
            <tr>
                <td><%= index + 1%></td>
                <td><%= user.username%></td>
                <td><%= user.phoneNo%></td>
                <td><%= user.privilege%></td>
                <td>
                  <a href="#"><span class="glyphicon glyphicon-pencil"></span></a>
              	  <a href="#"><span class="glyphicon glyphicon-trash"></span></a>
                </td>
            </tr>
            <% }) %>
        </tbody>
      </table>
      <button type="button" class="btn btn-success">
        <span class="glyphicon glyphicon-plus"></span> 添加用户
      </button>
     <br class="clearfloat"/>
    </div><!--end #mainContent-->
<%- include footer%>